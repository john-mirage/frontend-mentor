{
  "name": "age-calculator-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/age-calculator-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/age-calculator-app"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "age-calculator-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "age-calculator-app:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "age-calculator-app:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "age-calculator-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "age-calculator-app:build:development"
        },
        "production": {
          "buildTarget": "age-calculator-app:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/apps/age-calculator-app"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/age-calculator-app"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
